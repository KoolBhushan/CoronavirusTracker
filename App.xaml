<Application x:Class="CoronovirusTracker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:CoronovirusTracker.Views"
             xmlns:viewmodels="clr-namespace:CoronovirusTracker.ViewModels"
             xmlns:local="clr-namespace:CoronovirusTracker"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:HomeViewModel}">
            <views:HomeView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:StateViewModel}">
            <views:StateView/>
        </DataTemplate>
    </Application.Resources>
</Application>
